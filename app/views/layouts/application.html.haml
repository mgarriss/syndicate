!!! XML
!!!
%html
  %head
    %title= @title ? "syndicate - #{@title}" : "syndicate"
    = stylesheet_link_tag :all
    = javascript_include_tag :defaults
    = csrf_meta_tag
    = stylesheet_link_tag 'compiled/screen.css', :media => 'screen, projection'
    = stylesheet_link_tag 'compiled/print.css', :media => 'print'
    /[if lt IE 8]
      = stylesheet_link_tag 'compiled/ie.css', :media => 'screen, projection'
    = yield(:head)
  %body.bp
    #container
      #header
        .right.spaced= render 'shared/links'
        %h1= link_to('syndicate', root_path)
        %ul
          %li
            %h3= link_to('users', users_path) + ' |'
          %li
            %h3= link_to('regions', regions_path) + ' .'
          %li
            %h3= link_to('systems', systems_path) + ' .'
          %li
            %h3= link_to('stations', stations_path) + ' |'
          %li
            %h3= link_to('item types', inventory_types_path) + ' .'
      #content= yield
      #footer
        -#= session.inspect
